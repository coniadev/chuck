{
    "name": "conia/chuck",
    "description": "A lightweight PHP framework",
    "keywords": [
        "framework",
        "micro",
        "api",
        "router"
    ],
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "phpunit/phpunit": "^9",
        "pestphp/pest": "^1",
        "dealerdirect/phpcodesniffer-composer-installer": "^1",
        "vimeo/psalm": "^5",
        "nyholm/psr7-server": "^1.0",
        "nyholm/psr7": "^1.5"
    },
    "require": {
        "php": ">=8.1",
        "conia/cli": "dev-main",
        "laminas/laminas-httphandlerrunner": "^2.4",
        "psr/http-factory": "^1.0",
        "psr/http-message": "^1.0",
        "psr/http-server-handler": "^1.0",
        "psr/http-server-middleware": "^1.0",
        "psr/log": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Conia\\Chuck\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Conia\\Chuck\\Tests\\": "tests"
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
