{
    "name": "ebenefuenf/chuck",
    "description": "",
    "version": "0.1.0",
    "type": "project",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "psy/psysh": "*",
        "phpunit/phpunit": "^9",
        "nunomaduro/collision": "^6",
        "pestphp/pest": "^1",
        "filp/whoops": "^2.14",
        "dealerdirect/phpcodesniffer-composer-installer": "^0.7.2",
        "sirbrillig/phpcs-variable-analysis": "^2.11"
    },
    "require": {
        "tgalopin/html-sanitizer": "^1.5",
        "monolog/monolog": "^2.3",
        "league/plates": "^3.4"
    },
    "autoload": {
        "psr-4": {
            "Chuck\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Chuck\\Tests\\": "tests/setup"
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
