{
    "name": "ebenefuenf/chuck",
    "description": "",
    "version": "0.1.0",
    "type": "project",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "phpunit/phpunit": "^9",
        "nunomaduro/collision": "^6",
        "pestphp/pest": "^1",
        "dealerdirect/phpcodesniffer-composer-installer": "^0.7.2",
        "sirbrillig/phpcs-variable-analysis": "^2.11",
        "vimeo/psalm": "^5.0.0-alpha1",
        "league/plates": "^3.4"
    },
    "require": {
        "php": ">=8.1",
        "tgalopin/html-sanitizer": "^1.5"
    },
    "suggest": {
        "league/plates": "If you want to use the Plates template engine"
    },
    "autoload": {
        "psr-4": {
            "Chuck\\": "src",
            "Chuck\\Lib\\": "lib"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Chuck\\Tests\\": "tests/setup",
            "Chuck\\Tests\\Fix\\": "tests/fixtures"
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
